%tr
  %td= question.question_text
  %td= link_to "#{t('admin.questions.index.answers')}(#{question.answer_count})", admin_question_answers_path(question_id: question.to_param)
  %td= question.created_at.to_s(:long)
  %td= link_to t('common.delete'), admin_question_path(id: question), confirm: t('common.confirm'), :method => :delete
